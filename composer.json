{
  "name": "webcrafts-studio/lens-for-laravel",
  "description": "A plug-and-play accessibility auditor for Laravel.",
  "type": "library",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/routing": "^10.0|^11.0|^12.0",
    "laravel/ai": "^0.1.0",
    "spatie/browsershot": "^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "LensForLaravel\\LensForLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LensForLaravel\\LensForLaravel\\LensForLaravelServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "laravel/pint": "^1.27",
    "laravel/boost": "^2.2",
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^10.0",
    "pestphp/pest": "^3.0",
    "pestphp/pest-plugin-laravel": "^3.0"
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "pint": "vendor/bin/pint"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
